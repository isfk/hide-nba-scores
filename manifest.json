{
    "name": "Hide NBA scores",
    "version": "0.0.1",
    "manifest_version": 3,
    "description": "An extension to hide tencent nba scores.",
    "icons": {
        "16": "icons/hide16x16.png",
        "32": "icons/hide32x32.png",
        "48": "icons/hide48x48.png",
        "128": "icons/hide128x128.png"
    },
    "action": {
        "default_popup": "popup/popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "lib/jquery.min.js",
                "content-scripts/content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "images/*.png"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}